<UserControl
    x:Class="TestDialog.Views.MessageBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:prism="http://prismlibrary.com/"
    Width="200"
    Height="150"
    prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel>
            <TextBlock FontSize="20" Text="{Binding Data.Title}" />
            <TextBlock FontSize="20" Text="{Binding Data.Content}" />
            <Button
                Width="50"
                Height="20"
                Command="{Binding CommCloseDialogand}"
                Content="click me!" />
        </StackPanel>
    </Grid>
</UserControl>
